---
import astroSvg from "../assets/design/astro.svg";
import backgroundGalaxy from "../assets/design/galaxy.webp";
import { Image } from "astro:assets";
import KeyboardText from "../components/elements/KeyboardText.astro";
import cube from "../assets/svg/cube.svg";
import circleLines from "../assets/svg/circle-lines.svg";
import CubeSvg from "../components/elements/cubeSvg.astro";
import student from "../assets/icons/graduated.svg";
import developper from "../assets/icons/programmer.svg";
import job from "../assets/icons/job.svg";
---

<section
  class="w-screen md:flex hidden h-screen relative items-center justify-center flex-col mt-10"
>
  <div class="w-full flex items-center justify-center flex-col relative z-0">
    <h2
      class="lg:text-7xl text-5xl font-semibold text-white flex items-end space-x-4 justify-center"
    >
      <span>Harness</span>
      <span class="relative -z-10 flex md:mb-2 items-center justify-center">
        <Image
          src={backgroundGalaxy}
          alt=""
          class="md:w-40 w-20 h-auto rounded-full"
        />
        <Image src={astroSvg} alt="" class="w-52 h-auto absolute bottom-0" />
      </span>
      <span>Africa's</span>
    </h2>
    <KeyboardText />
    <h2
      class="lg:text-7xl text-5xl relative font-semibold text-white flex items-end space-x-4 justify-center"
    >
      <span
        class="absolute -z-50 -right-6 -bottom-3.5 lg:h-24 lg:w-24 w-20 h-20"
      >
        <svg
          width="100%"
          height="100%"
          viewBox="0 0 200 200"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
        >
          <g clip-path="url(#clip0_104_44)">
            <path
              fill-rule="evenodd"
              clip-rule="evenodd"
              d="M120 0H80V51.7157L43.4315 15.1472L15.1472 43.4314L51.7158 80H0V120H51.7157L15.1472 156.568L43.4315 184.853L80 148.284V200H120V148.284L156.569 184.853L184.853 156.569L148.284 120H200V80H148.284L184.853 43.4314L156.569 15.1471L120 51.7157V0Z"
              fill="url(#paint0_linear_104_44)"></path>
          </g>
          <defs>
            <linearGradient
              id="paint0_linear_104_44"
              x1="100"
              y1="0"
              x2="100"
              y2="200"
              gradientUnits="userSpaceOnUse"
            >
              <stop stop-color="#ff3200"></stop>
              <stop offset="1" stop-color="#ff7000"></stop>
            </linearGradient>
            <clipPath id="clip0_104_44">
              <rect width="200" height="200" fill="white"></rect>
            </clipPath>
          </defs>
        </svg>
      </span>
       Potential.
    </h2>
  </div>
  <div class="max-w-5xl mx-auto text-center px-8 relative">
    <p class="text-gray-100 pt-8 md:text-xl text-lg mix-blend-difference">
      Lorem ipsum dolor sit amet consectetur adipisicing elit. Beatae ducimus
      eligendi repellendus voluptatibus maxime sequi, nemo fugit excepturi
      consequuntur!
    </p>
  </div>
  <div
    class="max-w-4xl relative mx-auto flex items-center justify-center mt-8 space-x-4"
  >
    <button
      class="bg-gradient-to-b from-[#ff3200] to-[#ff6000] hover:bg-orange-500 text-white font-medium py-3.5 px-8 rounded-full"
    >
      Join Community
    </button>
    <button
      class="w-14 h-14 rounded-full bg-white flex items-center justify-center"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        viewBox="0 0 24 24"
        fill="#161616"
        class="w-6 h-6"
      >
        <path
          fill-rule="evenodd"
          d="M4.5 5.653c0-1.426 1.529-2.33 2.779-1.643l11.54 6.348c1.295.712 1.295 2.573 0 3.285L7.28 19.991c-1.25.687-2.779-.217-2.779-1.643V5.653z"
          clip-rule="evenodd"></path>
      </svg>
    </button>
  </div>
  <section class="w-full -mb-20 pt-[3vw] px-8">
    <div
      class="max-w-7xl relative border-2 rounded-full mx-auto flex items-center justify-between px-8 py-2"
    >
      <div class="flex items-center justify-center gap-x-6 w-full">
        <div class="flex space-x-2 items-center justify-center">
          <Image src={student} alt="student Arkx" class="w-8 h-auto" />
          <div class="text-white">
            <p class="font-medium">+600</p>
            <p class="text-sm">Alumnis</p>
          </div>
        </div>
        <div class="flex space-x-2 items-center justify-center">
          <Image src={student} alt="student Arkx" class="w-8 h-auto" />
          <div class="text-white">
            <p class="font-medium">+5000</p>
            <p class="text-sm">Initiated to coding</p>
          </div>
        </div>
        <div class="flex space-x-2 items-center justify-center">
          <Image src={job} alt="student Arkx" class="w-8 h-auto" />
          <div class="text-white">
            <p class="font-medium">+87%</p>
            <p class="text-sm">Insertion rate</p>
          </div>
        </div>
      </div>
      <div class="w-full h-full absolute flex items-center justify-center">
        <svg
          width="6vw"
          height="6vw"
          viewBox="0 0 200 200"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
        >
          <g clip-path="url(#clip0_103_15)">
            <path
              d="M93.6797 5.15479C97.359 2.15401 102.641 2.15401 106.32 5.15479L114.253 11.625C116.75 13.6608 120.074 14.3696 123.183 13.5291L133.039 10.8652C137.616 9.62811 142.432 11.775 144.572 16.0058L149.229 25.2154C150.679 28.0814 153.416 30.0768 156.588 30.5793L166.717 32.184C171.384 32.9234 174.897 36.829 175.139 41.5483L175.672 51.9384C175.836 55.133 177.519 58.0565 180.199 59.8033L188.865 65.452C192.811 68.0247 194.427 73.0033 192.743 77.4035L189.015 87.1465C187.874 90.1271 188.225 93.4731 189.958 96.1526L195.618 104.899C198.175 108.852 197.629 114.053 194.306 117.389L186.976 124.748C184.722 127.011 183.684 130.214 184.184 133.369L185.815 143.668C186.552 148.327 183.933 152.868 179.531 154.564L169.921 158.264C166.93 159.416 164.672 161.931 163.849 165.029L161.188 175.047C159.973 179.623 155.716 182.719 150.989 182.466L140.78 181.92C137.567 181.748 134.468 183.132 132.452 185.639L126.014 193.647C123.04 197.346 117.876 198.445 113.653 196.277L104.567 191.612C101.7 190.141 98.2999 190.141 95.4331 191.612L86.3465 196.277C82.1243 198.445 76.9601 197.346 73.9862 193.647L67.548 185.639C65.5321 183.132 62.433 181.748 59.2203 181.92L49.0111 182.466C44.284 182.719 40.0271 179.623 38.8119 175.047L36.1507 165.029C35.3279 161.931 33.0702 159.416 30.0792 158.264L20.4691 154.564C16.0666 152.868 13.4477 148.327 14.1855 143.668L15.8162 133.369C16.3157 130.214 15.2781 127.011 13.024 124.748L5.69367 117.389C2.37113 114.053 1.82509 108.852 4.38248 104.899L10.0415 96.1526C11.7752 93.4731 12.1257 90.1271 10.9851 87.1465L7.25661 77.4035C5.57276 73.0033 7.18856 68.0247 11.1354 65.452L19.8014 59.8033C22.4812 58.0565 24.1636 55.133 24.3275 51.9384L24.8607 41.5483C25.1029 36.829 28.6155 32.9234 33.2828 32.184L43.4118 30.5793C46.5838 30.0768 49.3214 28.0814 50.7708 25.2154L55.4283 16.0058C57.5679 11.775 62.3844 9.6281 66.9613 10.8652L76.8169 13.5291C79.9264 14.3696 83.2504 13.6608 85.7465 11.625L93.6797 5.15479Z"
              fill="url(#paint0_linear_103_15)"></path>
          </g>
          <defs>
            <linearGradient
              id="paint0_linear_103_15"
              x1="177"
              y1="-9.23648e-06"
              x2="39.5"
              y2="152.5"
              gradientUnits="userSpaceOnUse"
            >
              <stop stop-color="#330072"></stop>
              <stop offset="1" stop-color="#5c00f9"></stop>
            </linearGradient>
            <clipPath id="clip0_103_15">
              <rect width="200" height="200" fill="white"></rect>
            </clipPath>
          </defs>
        </svg>
        
      </div>
      <h2 class="font-medium text-3xl text-white text-center w-full">
        5 Years of Impact
      </h2>
    </div>
  </section>
</section>
